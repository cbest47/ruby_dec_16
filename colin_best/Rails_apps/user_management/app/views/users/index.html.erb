<h1>See your contacts below!</h1>
<table>
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email Address</th>
    <tr>
  </thead>
<% @user.each do |user| %>
<tr>
  <td>
    <%= user.first_name %>
  </td>
  <td>
    <%= user.last_name %>
  </td>
  <td>
    <%= user.email_address %>
  </td>
  <td><%= link_to "show", user_path(user.id) %>
      <%= link_to "edit", edit_user_path(user.id) %>
      <%= link_to "delete", user_path(user.id), method: :delete %>

  </td>

</tr>
<% end %>
</table>
<%= link_to "Create", '/users/new' %>
